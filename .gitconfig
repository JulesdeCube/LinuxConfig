[core]
	excludesfile = ~/.gitignore
[color]
	ui = true
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[commit]
	verbose = true
[pull]
	rebase = true
[branch]
	autosetuprebase = always
[push]
	default = simple
[rebase]
	autoSquash = true
	autoStash = true
[sendemail]
	smtpencryption = tls
	smtpserver = smtp.office365.com
	smtpserverport = 587
	smtpuser = firstname.lastname@epita.fr
[user]
	name = 
	email = 
[alias]
	tree = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	lg = log --abbrev-commit --decorate --format=format:'%C(bold blue)%h %C(bold red)<%an> %C(bold green)(%ar)%C(reset) %C(white)%s%C(bold yellow)%d%C(reset)' --all 

